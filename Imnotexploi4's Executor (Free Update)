-- Create buttons with improved style
local function createButton(name, text, position, size, color, hoverColor)
local button = Instance.new("TextButton")
button.Name = name
button.BackgroundColor3 = color
button.Text = text
button.TextColor3 = Color3.fromRGB(255, 255, 255)
button.Font = Enum.Font.Gotham
button.TextSize = 18
button.Position = position
button.Size = size
button.Parent = mainFrame

local buttonCorner = Instance.new("UICorner")
buttonCorner.CornerRadius = UDim.new(0, 12)
buttonCorner.Parent = button

local function onMouseEnter()
button.BackgroundColor3 = hoverColor
end

local function onMouseLeave()
button.BackgroundColor3 = color
end

button.MouseEnter:Connect(onMouseEnter)
button.MouseLeave:Connect(onMouseLeave)

return button
end

local executeButton = createButton("ExecuteButton", "Execute", UDim2.new(0, 20, 1, -170), UDim2.new(0, 180, 0, 50), Color3.fromRGB(0, 128, 0), Color3.fromRGB(0, 100, 0))
local saveButton = createButton("SaveButton", "Save Script", UDim2.new(0.5, -90, 1, -170), UDim2.new(0, 180, 0, 50), Color3.fromRGB(0, 191, 255), Color3.fromRGB(0, 140, 210))
local copyButton = createButton("CopyButton", "Copy", UDim2.new(1, -200, 1, -170), UDim2.new(0, 180, 0, 50), Color3.fromRGB(255, 165, 0), Color3.fromRGB(204, 130, 0))
local clearButton = createButton("ClearButton", "Clear", UDim2.new(0, 20, 1, -240), UDim2.new(0, 180, 0, 50), Color3.fromRGB(255, 69, 0), Color3.fromRGB(204, 50, 0))
local loadButton = createButton("LoadButton", "Load Script", UDim2.new(1, -200, 1, -240), UDim2.new(0, 180, 0, 50), Color3.fromRGB(112, 128, 144), Color3.fromRGB(80, 100, 114))

