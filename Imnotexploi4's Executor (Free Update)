-- Function to make GUIs draggable
local function makeDraggable(gui)
    local dragging, dragInput, dragStart, startPos
    
    local function update(input)
        local delta = input.Position - dragStart
        gui.Position = UDim2.new(
            startPos.X.Scale, startPos.X.Offset + delta.X,
            startPos.Y.Scale, startPos.Y.Offset + delta.Y
        )
    end
    
    gui.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = gui.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    
    gui.InputChanged:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            update(input)
        end
    end)
end

makeDraggable(mainFrame)
makeDraggable(keyFrame)

local function createPageFile(name)
    local pageFile = Instance.new("Frame", pageContainer)
    pageFile.Name = name
    pageFile.Size = UDim2.new(1, 0, 1, 0)
    pageFile.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
    
    local scriptLabel = Instance.new("TextLabel", pageFile)
    scriptLabel.Size = UDim2.new(0.9, 0, 0, 30)
    scriptLabel.Position = UDim2.new(0.05, 0, 0, 10)
    scriptLabel.BackgroundTransparency = 1
    scriptLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    scriptLabel.Font = Enum.Font.GothamBold
    scriptLabel.TextSize = 24
    scriptLabel.Text = name
    
    local codeInput = Instance.new("TextBox", pageFile)
    codeInput.Size = UDim2.new(0.9, 0, 0.6, 0)
    codeInput.Position = UDim2.new(0.05, 0, 0.25, 10)
    codeInput.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
    codeInput.TextColor3 = Color3.fromRGB(255, 255, 255)
    codeInput.Text = ""
    codeInput.ClearTextOnFocus = false
    codeInput.MultiLine = true
    codeInput.TextWrapped = true
    codeInput.Font = Enum.Font.Code
    codeInput.TextSize = 20
    codeInput.PlaceholderText = "Enter your script here..."
    
    -- Add New Buttons
    local clearButton = Instance.new("TextButton", pageFile)
    clearButton.Size = UDim2.new(0, 60, 0, 30)
    clearButton.Position = UDim2.new(0.1, 0, 0.85, 0)
    clearButton.BackgroundColor3 = Color3.fromRGB(255, 69, 0)
    clearButton.Text = "Clear"
    clearButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    clearButton.Font = Enum.Font.GothamBold
    clearButton.TextSize = 14
    
    local loadButton = Instance.new("TextButton", pageFile) loadButton.Size = UDim2.new(0, 60, 0, 30)
    loadButton.Position = UDim2.new(0.25, 0, 0.85, 0)
    loadButton.BackgroundColor3 = Color3.fromRGB(0, 191, 255)
    loadButton.Text = "Load"
    loadButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    loadButton.Font = Enum.Font.GothamBold
    loadButton.TextSize = 14
    
    local saveButton = Instance.new("TextButton", pageFile)
    saveButton.Size = UDim2.new(0, 60, 0, 30)
    saveButton.Position = UDim2.new(0.4, 0, 0.85, 0)
    saveButton.BackgroundColor3 = Color3.fromRGB(0, 191, 255)
    saveButton.Text = "Save"
    saveButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    saveButton.Font = Enum.Font.GothamBold
    saveButton.TextSize = 14
    
    local copyButton = Instance.new("TextButton", pageFile)
    copyButton.Size = UDim2.new(0, 60, 0, 30)
    copyButton.Position = UDim2.new(0.55, 0, 0.85, 0)
    copyButton.BackgroundColor3 = Color3.fromRGB(0, 191, 255)
    copyButton.Text = "Copy"
    copyButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    copyButton.Font = Enum.Font.GothamBold
    copyButton.TextSize = 14
    
    local executeButton = Instance.new("TextButton", pageFile)
    executeButton.Size = UDim2.new(0, 60, 0, 30)
    executeButton.Position = UDim2.new(0.7, 0, 0.85, 0)
    executeButton.BackgroundColor3 = Color3.fromRGB(0, 255, 127)
    executeButton.Text = "Execute"
    executeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    executeButton.Font = Enum.Font.GothamBold
    executeButton.TextSize = 14

    local deleteButton = Instance.new("TextButton", pageFile)
    deleteButton.Size = UDim2.new(0, 40, 0, 40)
    deleteButton.Position = UDim2.new(1, -50, 0, 10)
    deleteButton.BackgroundColor3 = Color3.fromRGB(220, 20, 60)
    deleteButton.Text = "X"
    deleteButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    deleteButton.Font = Enum.Font.GothamBold
    deleteButton.TextSize = 18

    local pageFileCorner = Instance.new("UICorner", pageFile) pageFileCorner.CornerRadius = UDim.new(0, 15)

    clearButton.MouseButton1Click:Connect(function()
        codeInput.Text = ""
    end)

    loadButton.MouseButton1Click:Connect(function()
        if savedScripts[name] then
            codeInput.Text = HttpService:JSONDecode(savedScripts[name])
        else
            codeInput.Text = "No script saved for this file."
        end
    end)

    saveButton.MouseButton1Click:Connect(function()
        savedScripts[name] = HttpService:JSONEncode(codeInput.Text)
    end)

    copyButton.MouseButton1Click:Connect(function()
        setclipboard(codeInput.Text)
    end)

    executeButton.MouseButton1Click:Connect(function()
        loadstring(codeInput.Text)()
    end)

    deleteButton.MouseButton1Click:Connect(function()
        pageFile:Destroy()
        table.remove(pageFiles, table.find(pageFiles, pageFile))
    end)

    table.insert(pageFiles, pageFile)
end

local function createNewPage()
    local pageCount = #pageFiles + 1
    createPageFile("script" .. pageCount .. ".lua")
end

