-- Script for Imnotexploi4's Executor V0.7

-- Variables
local player = game.Players.LocalPlayer
local gui = Instance.new("ScreenGui", player.PlayerGui)
local mainFrame = Instance.new("Frame", gui)
local title = Instance.new("TextLabel", mainFrame)
local closeButton = Instance.new("TextButton", mainFrame)
local toggleButton = Instance.new("ImageButton", gui) -- Changed to ImageButton to display an image
local copyButton = Instance.new("TextButton", mainFrame)
local pasteButton = Instance.new("TextButton", mainFrame)
local executeButton = Instance.new("TextButton", mainFrame)
local scriptBox = Instance.new("TextBox", mainFrame)

-- URL for the Delta Exploit icon (Replace with the actual URL of the image)
local deltaExploitIconURL = "http://www.example.com/delta_exploit_icon.png"

-- GUI Properties
gui.Name = "Imnotexploi4ExecutorV07"
mainFrame.Name = "MainFrame"
mainFrame.Size = UDim2.new(0.5, 0, 0.5, 0)
mainFrame.Position = UDim2.new(0.25, 0, 0.25, 0)
mainFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
mainFrame.Active = true
mainFrame.Draggable = true

title.Name = "Title"
title.Size = UDim2.new(1, 0, 0.1, 0)
title.Position = UDim2.new(0, 0, 0, 0)
title.Text = "Imnotexploi4's Executor V0.7"
title.TextColor3 = Color3.fromRGB(0, 0, 0)
title.BackgroundColor3 = Color3.fromRGB(200, 200, 200)

closeButton.Name = "CloseButton"
closeButton.Size = UDim2.new(0.1, 0, 0.1, 0)
closeButton.Position = UDim2.new(0.9, 0, 0, 0)
closeButton.Text = "X"
closeButton.TextColor3 = Color3.fromRGB(255, 0, 0)
closeButton.BackgroundColor3 = Color3.fromRGB(200, 200, 200)

toggleButton.Name = "ToggleButton"
toggleButton.Size = UDim2.new(0.1, 0, 0.1, 0)
toggleButton.Position = UDim2.new(0, 0, 0, 0)
toggleButton.Image = deltaExploitIconURL -- Set the image for the toggle button
toggleButton.Visible = false
toggleButton.BackgroundColor3 = Color3.fromRGB(200, 200, 200)
toggleButton.Active = true
toggleButton.Draggable = true

copyButton.Name = "CopyButton"copyButton.Size = UDim2.new(0.2, 0, 0.1, 0)
copyButton.Position = UDim2.new(0.1, 0, 0.85, 0)
copyButton.Text = "Copy"
copyButton.BackgroundColor3 = Color3.fromRGB(200, 200, 200)

pasteButton.Name = "PasteButton"
pasteButton.Size = UDim2.new(0.2, 0, 0.1, 0)
pasteButton.Position = UDim2.new(0.7, 0, 0.85, 0)
pasteButton.Text = "Paste"
pasteButton.BackgroundColor3 = Color3.fromRGB(200, 200, 200)

executeButton.Name = "ExecuteButton"
executeButton.Size = UDim2.new(0.2, 0, 0.1, 0)
executeButton.Position = UDim2.new(0.4, 0, 0.85, 0)
executeButton.Text = "Execute"
executeButton.BackgroundColor3 = Color3.fromRGB(100, 200, 100)

scriptBox.Name = "ScriptBox"
scriptBox.Size = UDim2.new(0.9, 0, 0.65, 0)
scriptBox.Position = UDim2.new(0.05, 0, 0.1, 0)
scriptBox.Text = "Enter script here"

-- Functions
local function closeGUI()
    mainFrame.Visible = false
    toggleButton.Visible = true
end

local function openGUI()
    mainFrame.Visible = true
    toggleButton.Visible = false
end

local function copyScript()
    setclipboard(scriptBox.Text)
end

local function pasteScript()
    scriptBox.Text = game:GetService("UserInputService"):GetClipboard()
end

local function executeScript()
    local scriptToExecute = scriptBox.Text
    loadstring(scriptToExecute)()
end

-- Connection of Functions to Buttons
closeButton.MouseButton1Click:Connect(closeGUI)
toggleButton.MouseButton1Click:Connect(openGUI)
copyButton.MouseButton1Click:Connect(copyScript)
pasteButton.MouseButton1Click:Connect(pasteScript)
executeButton.MouseButton1Click:Connect(executeScript)

-- Welcome message
print("Hello " .. player.Name .. ", Welcome to Imnotexploi4's Executor V0.7")

-- Note: Ensure that the game allows the use of setclipboard and GetClipboard from UserInputService, as well as executing scripts through loadstring.
