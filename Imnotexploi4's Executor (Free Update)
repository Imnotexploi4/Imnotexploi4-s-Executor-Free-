scriptBox.Name = "ScriptBox"
scriptBox.Size = UDim2.new(0.9, 0, 0.5, 0)
scriptBox.Position = UDim2.new(0.05, 0, 0.4, 0)
scriptBox.Text = "Enter script here"
scriptBox.TextColor3 = Color3.fromRGB(255, 255, 255)
scriptBox.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
scriptBox.MultiLine = true

-- Functions
local function closeGUI()
mainFrame.Visible = false
toggleButton.Visible = true
end

local function openGUI()
mainFrame.Visible = true
toggleButton.Visible = false
end

local function copyScript()
setclipboard(scriptBox.Text)
end

local function pasteScript()
scriptBox.Text = game:GetService("UserInputService"):GetClipboard()
end

local function executeScript()
local scriptToExecute = scriptBox.Text
loadstring(scriptToExecute)()
end

local function clearScript()
scriptBox.Text = ""
end

local function loadScript()
-- Placeholder for loading script from a file or recent scripts
end

local function saveScript()
-- Placeholder for saving script to a file or recent scripts
end

local function onPlayerDied()
mainFrame.Visible = true
toggleButton.Visible = false
end

-- Connection of Functions to Buttons
closeButton.MouseButton1Click:Connect(closeGUI)
toggleButton.MouseButton1Click:Connect(openGUI)
copyButton.MouseButton1Click:Connect(copyScript)
pasteButton.MouseButton1Click:Connect(pasteScript)
executeButton.MouseButton1Click:Connect(executeScript)
clearButton.MouseButton1Click:Connect(clearScript)
loadButton.MouseButton1Click:Connect(loadScript)
saveButton.MouseButton1Click:Connect(saveScript)

-- Connect the player's death event
player.CharacterAdded:Connect(function(character)
local humanoid = character:WaitForChild("Humanoid")
humanoid.Died:Connect(onPlayerDied)
end)

-- Ensure connection on initial character
if player.Character then
local humanoid = player.Character:FindFirstChild("Humanoid")
if humanoid then
humanoid.Died:Connect(onPlayerDied)
end
end

-- Welcome message
print("Hello " .. player.Name .. ", Welcome to Imnotexploi4's Executor V0.8")

-- Note: Ensure that the game allows the use of setclipboard and GetClipboard from UserInputService, as well as executing scripts through loadstring.
