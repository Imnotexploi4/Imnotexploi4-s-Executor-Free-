toggleButton.Image = "rbxassetid://111767375753277" -- Imnotexploi4 new icon
toggleButton.Visible = false
toggleButton.Parent = screenGui
toggleButton.ZIndex = 10

-- Make UI Elements Draggable
local function makeDraggable(frame)
local dragging, dragInput, dragStart, startPos

local function update(input)
local delta = input.Position - dragStart
frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

frame.InputBegan:Connect(function(input)
if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
dragging = true
dragStart = input.Position
startPos = frame.Position

input.Changed:Connect(function()
if input.UserInputState == Enum.UserInputState.End then
dragging = false
end
end)
end
end)

frame.InputChanged:Connect(function(input)
if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
dragInput = input
end
end)

UserInputService.InputChanged:Connect(function(input)
if input == dragInput and dragging then
update(input)
end end)
end

-- Apply draggable functionality
makeDraggable(mainFrame)
makeDraggable(toggleButton)

-- Function to toggle main GUI visibility
local function toggleGUI()
mainFrame.Visible = not mainFrame.Visible
toggleButton.Visible = not mainFrame.Visible
end

-- Connect the close button click event
closeButton.MouseButton1Click:Connect(toggleGUI)

-- Connect the toggle button click event
toggleButton.MouseButton1Click:Connect(toggleGUI)

-- Function to execute code
local function executeCode()
local code = codeInput.Text

-- Safely execute the code inside a new environment
local function safeExecute(source)
local func, err = loadstring(source)
if func then
setfenv(func, getfenv())  -- Set the environment of the function to the current environment
local success, execErr = pcall(func)
if not success then
errorLabel.Text = "Error executing code: " .. execErr
errorLabel.Visible = true
else
errorLabel.Text = ""
errorLabel.Visible = false
end
else
errorLabel.Text = "Error parsing code: " .. err
errorLabel.Visible = true
end
end

safeExecute(code)
end

-- Function to copy code
local function copyCode()
if setclipboard then
setclipboard(codeInput.Text)
errorLabel.Text = "Code copied to clipboard"
errorLabel.Visible = true
else
errorLabel.Text = "setclipboard function is not available."
errorLabel.Visible = true
end
end

-- Function to clear code input
local function clearCode()
codeInput.Text = ""
errorLabel.Text = "Code input cleared"
errorLabel.Visible = true
end

-- Function to save code
local function saveCode()
-- Implement logic to save the code
errorLabel.Text = "Save functionality not implemented"
errorLabel.Visible = true
end

-- Function to load code
local function loadCode()
-- Implement logic to load the code
local loadedCode = "print('Hello, world!')" -- Replace with actual loaded content
codeInput.Text = loadedCode
errorLabel.Text = "Code loaded"
errorLabel.Visible = true
end

-- Connect button click events
executeButton.MouseButton1Click:Connect(executeCode) copyButton.MouseButton1Click:Connect(copyCode)
saveButton.MouseButton1Click:Connect(saveCode)
clearButton.MouseButton1Click:Connect(clearCode)
loadButton.MouseButton1Click:Connect(loadCode)

-- Adjust GUI size based on device type
local function adjustGUISize()
local screenSize = workspace.CurrentCamera.ViewportSize
if screenSize.Y <= 720 then
-- Mobile device - smaller size
mainFrame.Size = UDim2.new(0, 340, 0, 500)
mainFrame.Position = UDim2.new(0.5, -170, 0.5, -250)
titleLabel.Size = UDim2.new(1, -20, 0, 50)
titleLabel.Position = UDim2.new(0, 10, 0, 10)
codeInput.Size = UDim2.new(1, -20, 0, 200)
codeInput.Position = UDim2.new(0, 10, 0, 70)

for _, button in ipairs(mainFrame:GetChildren()) do
if button:IsA("TextButton") and button.Name ~= "CloseButton" then
button.Size = UDim2.new(0, 100, 0, 30)
end
end

executeButton.Position = UDim2.new(0, 10, 1, -90)
saveButton.Position = UDim2.new(0.33, 0, 1, -90)
copyButton.Position = UDim2.new(0.66, 0, 1, -90)
clearButton.Position = UDim2.new(0, 10, 1, -130)
loadButton.Position = UDim2.new(0.33, 0, 1, -130)
errorLabel.Position = UDim2.new(0, 10, 1, -40)
else
-- PC - normal size
mainFrame.Size = UDim2.new(0, 600, 0, 700)
mainFrame.Position = UDim2.new(0.5, -300, 0.5, -350)
titleLabel.Size = UDim2.new(1, -20, 0, 60)
titleLabel.Position = UDim2.new(0, 10, 0, 10)
codeInput.Size = UDim2.new(1, -40, 0, 350)
codeInput.Position = UDim2.new(0, 20, 0, 80)

for _, button in ipairs(mainFrame:GetChildren()) do
if button:IsA("TextButton") and button.Name ~= "CloseButton" then
button.Size = UDim2.new(0, 180, 0, 50)
end
end

executeButton.Position = UDim2.new(0, 20, 1, -170)
saveButton.Position = UDim2.new(0.5, -90, 1, -170)
copyButton.Position = UDim2.new(1, -200, 1, -170)
clearButton.Position = UDim2.new(0, 20, 1, -240)
loadButton.Position = UDim2.new(1, -200, 1, -240)
errorLabel.Position = UDim2.new(0, 10, 1, -80)
end
end

-- Initial GUI adjustment
adjustGUISize()

-- Connect to screen size change event
workspace.CurrentCamera:GetPropertyChangedSignal("ViewportSize"):Connect(adjustGUISize)

-- Welcome message
print("Hello " .. LocalPlayer.Name .. ", Welcome to Imnotexploi4's Executor V1.3")
errorLabel.Visible = false
