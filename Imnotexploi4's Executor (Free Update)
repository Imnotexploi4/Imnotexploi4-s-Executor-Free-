local recentScriptsFrame = Instance.new("Frame", mainFrame)
local recentScriptsTitle = Instance.new("TextLabel", recentScriptsFrame)
local recentScriptsList = Instance.new("ScrollingFrame", recentScriptsFrame)
local UIListLayout = Instance.new("UIListLayout", recentScriptsList)

